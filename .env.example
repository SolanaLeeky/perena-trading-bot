# RPC URL for Solana connection (Helius, QuickNode, etc.)
RPC_URL="https://your-rpc-provider-url-here"

# Base58 encoded private keys for your wallets (comma-separated for multiple wallets)
# Example for single wallet: PRIVATE_KEYS_BASE58="your_base58_private_key_here"
# Example for multiple wallets: PRIVATE_KEYS_BASE58="wallet1_key,wallet2_key,wallet3_key"
PRIVATE_KEYS_BASE58="your-base58-private-key-here"

# Discord Webhook Configuration (Optional)
# Set DISCORD_ENABLED=true to enable Discord notifications
# Get webhook URL from Discord server settings > Integrations > Webhooks
DISCORD_ENABLED=false
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_url_here

# Perena API session token - Get from Perena dashboard
SESSION_TOKEN="your-session-token-here"